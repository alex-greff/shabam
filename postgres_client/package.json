{
    "name": "postgress_client",
    "version": "1.0.0",
    "description": "The docker image for running the postgres client.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "bash ./scripts/build.sh",

        "start:it--main": "bash ./scripts/start_it.sh 'db-main' postgres postgres development 5432",
        "start:psql--main": "bash ./scripts/start_psql.sh 'db-main' postgres postgres development 5432",
        "exec:init-schema--main": "bash ./scripts/exec_init_schema.sh '/app/scripts/db/main/initialize_schema.sh' 'db-main' postgres postgres development 5432"
    },
