{
    "name": "postgress_client",
    "version": "1.0.0",
    "description": "The docker image for running the postgres client.",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "bash ./scripts/build.sh",

        "start:it--metadata": "bash ./scripts/start_it.sh 'db-metadata' postgres postgres development 5432",
        "start:psql--metadata": "bash ./scripts/start_psql.sh 'db-metadata' postgres postgres development 5432",
        "exec:init-schema--metadata": "bash ./scripts/actions/init_metadata_db.sh false",
        "exec:init-schema--address-all": "bash ./scripts/actions/init_all_address_dbs.sh 3 false",
        "exec:init-schema--address-1": "bash ./scripts/actions/init_address_db.sh 1 false",
        "exec:init-schema--address-2": "bash ./scripts/actions/init_address_db.sh 2 false",
        "exec:init-schema--address-3": "bash ./scripts/actions/init_address_db.sh 3 false"
    },
    "author": "Alexander Greff",
    "license": "ISC"
}
