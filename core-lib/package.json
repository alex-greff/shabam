{
  "name": "@shabam/core-lib",
  "version": "1.0.0",
  "description": "Core algorithm functionality for Shabam.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "gypfile": true,
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "build:native": "node-gyp configure && node-gyp build",
    "test:memory-engine": "node --loader ts-node/esm --experimental-specifier-resolution=node ./tests/memory-engine.tests.ts",
    "test:native-spectrogram": "node --loader ts-node/esm --experimental-specifier-resolution=node ./tests/native-spectrogram.tests.ts",
    "test:temp": "node --loader ts-node/esm --experimental-specifier-resolution=node ./tests/temp.tests.ts",
    "test:clear-cache": "rm ./tests/data/*.cache.bson"
  },
  "author": "alex-greff",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@descript/web-audio-js": "^0.15.3-patched.5",
    "@types/d3": "^7.4.0",
    "browser-or-node": "2.0.0",
    "bson": "4.6.4",
    "d3": "^7.8.2",
    "node-addon-api": "5.0.0",
    "tsafe": "0.9.0",
    "wave-resampler": "^1.0.0"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.1",
    "@types/node": "17.0.5",
    "@types/node-wav": "0.0.0",
    "@types/web": "0.0.89",
    "canvas": "^2.11.0",
    "jsdom": "^21.1.0",
    "rollup": "2.62.0",
    "rollup-plugin-native": "1.2.16",
    "rollup-plugin-natives": "0.7.5",
    "rollup-plugin-typescript2": "0.31.1",
    "ts-node": "10.7.0",
    "typescript": "4.5.4"
  }
}
